            "outputPath": "dist/mfe-user",
            "index": "projects/mfe-user/src/index.html",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/mfe-user/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/mfe-user/public",
