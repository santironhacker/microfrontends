            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4301,
            "publicHost": "http://localhost:4301",
            "extraWebpackConfig": "projects/mfe-user/webpack.config.js"
          }
        },
        "extract-i18n": {
